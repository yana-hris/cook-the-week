{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "CookTheWeekDbContextConnection": "My_Connection_String"
  },
  "SendGrid": {
    "CookTheWeek_API_Key": "My_SendGrid_API_Key"
  },
  "EmailSettings": {
    "FromEmail": "My_From_Email",
    "ToEmail": "My_To_Email"
  },
  "IdentityOptions": {
    "Lockout": {
      "AllowedForNewUsers": true,
      "DefaultLockoutTimeSpan": 5,
      "MaxFailedAccessAttempts": 3
    }
  },
  "Rotativa": {
    "WkhtmlPath": "wwwroot/Rotativa/wkhtmltopdf.exe"
  }
}