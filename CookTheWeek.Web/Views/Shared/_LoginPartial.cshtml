@inject IUserContext UserContext
@{
    string userId = UserContext.UserId;
    bool isAdmin = UserContext.IsAdmin;

    if (userId != string.Empty && !isAdmin)
    {
        <partial name="_UserNavigationPartial" />
    }
    else if (userId != string.Empty && isAdmin)
    {
        <partial name="_AdminNavigationPartial" />
    }
    else
    {
        <partial name="_GuestNavigationPartial" />
    }
}


